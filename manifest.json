{
    "name": "확프",
    "description": "확프만들기",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/icon/1.png",
        "32": "/icon/1.png",
        "48": "/icon/1.png",
        "128": "/icon/1.png"
      }
    },
    "icons": {
      "16": "/icon/1.png",
      "32": "/icon/1.png",
      "48": "/icon/1.png",
      "128": "/icon/1.png"
    }
  }